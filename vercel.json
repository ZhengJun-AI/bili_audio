{
  "functions": {
    "src/app/api/download/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/info/route.ts": {
      "maxDuration": 15
    },
    "src/app/api/resolve-url/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/_vercel/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}